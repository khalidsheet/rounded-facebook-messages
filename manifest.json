{
    "name": "Rounded Facebook Messages",
    "version": "1.0",
    "description": "Make the messages in facebook web chat rounded. Again!",
    "manifest_version": 2,
	"browser_action": {
	  "default_popup": "index.html"
	 },
	"content_scripts": [
	  {
	    "matches": [
	      "*://www.facebook.com/*",
	      "*://facebook.com/*",
	      "*://web.facebook.com/*"
	    ],
	    "js": ["assets/index.js"],
	    "css": ["assets/index.css"]
	  }
	]
  }